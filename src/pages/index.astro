---
import Checkmark from '../components/Checkmark.astro';
import Layout from '../layouts/Layout.astro';
import kettlebells from '../assets/kettlebell-workout-group-training.jpg';
import { Picture } from 'astro:assets';
---

<Layout
  meta={{
    title:
      'Content marketing expertise for growing health &; wellness SaaS companies',
    description:
      'With a combined 30 years of experience in the tech industry, we help your health & wellness SaaS business grow organically.',
  }}
>
  <article id="main" class="relative z-10">
    <section class="region wrapper">
      <div class="switcher relative">
        <svg
          viewBox="0 0 256 256"
          version="1.1"
          xmlns="http://www.w3.org/2000/svg"
