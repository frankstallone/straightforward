---
import Checkmark from '../components/Checkmark.astro';
import Layout from '../layouts/Layout.astro';
import kettlebells from '../assets/kettlebell-workout-group-training.jpg';
import { Picture } from 'astro:assets';
import Bullet from '../components/Bullet.astro';
import AccordionItem from '../components/AccordionItem.astro';

const addOns = [
  'Content implementation',
  'Concierge Google Search Console service',
  'Concierge Google Ads service',
  'Concierge Google Analyics service',
  'Concierge Google Business service',
  'Comprehensive AEC reporting',
  'Rank tracking for insights',
  'Page level optimizations',
  'Branded mini sites',
  'Web performance audits',
  'More ...',
];

const colorWheel = {
  blue: 'bg-[#449deb] text-blue-vivid-900',
  mint: 'bg-[#4ce1bc] text-mint-vivid-900',
  gold: 'bg-[#ebc644] text-gold-vivid-900',
  indigo: 'bg-[#869df9] text-indigo-vivid-900',
  orange: 'bg-[#e1744c] text-orange-vivid-900',
  yellow: 'bg-[#dee14c] text-gold-vivid-900',
};
---

<Layout
  meta={{
    title:
      'Content marketing expertise for growing health & wellness SaaS companies',
    description:
      'With a combined 30 years of experience in the tech industry, we help your health & wellness SaaS business grow organically.',
  }}
>
  <article id="main" class="relative z-10">
    <section class="region wrapper" id="DriveLeads">
      <div class="switcher relative">
        <svg
          viewBox="0 0 256 256"
          version="1.1"
          xmlns="http://www.w3.org/2000/svg"
          class="absolute -inset-s z-0 h-4xl text-[#bfff00] hidden md:block"
          aria-hidden="true"
        >
          <path
            d="M121.182,0.951l134.441,134.438c-0.304,5.555 -0.968,11.082 -1.996,16.551l-149,-148.996c5.469,-1.024 11,-1.692 16.555,-1.993Zm41.734,4.645c11.844,3.352 23.137,8.398 33.536,14.984l39.539,39.539c6.586,10.395 11.632,21.692 14.988,33.532l-88.063,-88.055Zm-86.586,6.141l168.504,168.5c-1.867,4.207 -3.957,8.304 -6.265,12.285l-174.52,-174.52c3.977,-2.308 8.078,-4.398 12.281,-6.265Zm-33.738,21.902l180.34,180.336c-2.934,3.246 -6.027,6.344 -9.277,9.273l-180.336,-180.332c2.929,-3.25 6.027,-6.343 9.273,-9.277Zm-24.91,30.73l174.52,174.516c-3.977,2.309 -8.079,4.399 -12.282,6.266l-168.504,-168.5c1.868,-4.203 3.957,-8.305 6.266,-12.282Zm-15.059,40.579l149,149c-5.468,1.023 -11,1.687 -16.554,1.988l-134.442,-134.434c0.305,-5.558 0.969,-11.086 1.996,-16.554Zm2.649,58.289l88.058,88.058c-11.839,-3.351 -23.132,-8.402 -33.531,-14.988l-0.008,-0l-39.535,-39.531c-6.586,-10.399 -11.633,-21.696 -14.988,-33.539l0.004,-0Z"
            fill="currentColor"></path>
        </svg>
        <div class="box">
          <Picture
            src={kettlebells}
            widths={[240, 540, 720, kettlebells.width]}
            sizes={`(max-width: 360px) 240px, (max-width: 720px) 540px, (max-width: 1600px) 720px, ${kettlebells.width}px`}
            alt="Focused workout group training with kettlebells"
            quality={80}
            formats={['avif', 'webp']}
            class="relative h-auto w-auto mx-auto object-contain rounded-2xl row-start-1 row-span-6 col-start-1 col-span-6 border-[#e1744c] border-8 z-10"
            pictureAttributes={{
              class:
                'relative h-auto w-auto mx-auto object-contain rounded-2xl row-start-1 row-span-6 col-start-1 col-span-6 z-10',
            }}
          />
        </div>
        <div class="box flow prose">
          <h2 class="lg:text-step-4 leading-flat">
            Drive more inbound leads from your ideal audience
          </h2>
          <p>
            Regularly posting content, but just not attracting your tribe? Or
            know that content marketing is crucial for your business, but don't
            have the time or resources to do it well? At Straightforward Growth
            we believe in the power of research-driven content, and have a
            proven, repeatable model that will get your website (and your
            contact details) seen and used.
          </p>
        </div>
      </div>
    </section>
    <section class="region space-y-l-xl" id="HowWeDoIt">
      <div class="box w-fit mx-auto text-center">
        <h2 class="relative lg:text-[4vw]">
          <svg
