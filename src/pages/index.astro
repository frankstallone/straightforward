  <script>
    import Alpine from 'alpinejs';

    const formatter = new Intl.NumberFormat('en-US', {
      style: 'currency',
      currency: 'USD',
      maximumFractionDigits: 0,
    });

    window.Alpine = Alpine;

    Alpine.data('valueSlider', () => ({
      init() {
        this.calculateRevenue();
      },
      impressionsSlider: '5000',
      prettyImpressions: '0',
      prettyAverageValue: '0',
      leads: 0,
      leadsClosed: 0,
      averageValue: '120',
      potentialLTV: '$0',
      calculateRevenue() {
        this.leads = Math.round(
          (((Number(this.impressionsSlider) / 100) * 2) / 100) * 7
        );
        this.leadsClosed = Math.round((this.leads / 100) * 10);
        this.potentialLTV = formatter.format(
          Math.round(this.leadsClosed * Number(this.averageValue) * 36)
        );
        this.prettyImpressions = Number(this.impressionsSlider).toLocaleString(
          'en-US'
        );
        this.prettyAverageValue = Number(this.averageValue).toLocaleString(
          'en-US'
        );
      },
    }));

    Alpine.start();
  </script>
